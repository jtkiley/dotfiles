[include]
	path = ~/Dropbox/_config/gitconfig
[alias]
	branches = branch -a
[core]
	editor = atom --wait
	excludesfile = ~/.gitignore
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[push]
	default = simple
